%h2 Вход

= semantic_form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
  = f.error_messages
  = f.inputs do
    = f.input :email
    = f.input :password
    = f.input :remember_me, :as => :boolean, :checked=>true
  = f.buttons do
    = f.commit_button "Войти"

= render :partial => "devise/shared/links"
